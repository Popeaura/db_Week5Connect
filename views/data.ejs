<table>
    <thead>
        <tr>
            <th> Patient ID </th>
            <th> First Name </th>
            <th>Last Name </th>
            <th> Date of Birth  </th>
            <th> Gender</th>
            <th> language</th>
        </tr>
    </thead>
    <tbody>
        <% results.forEach(row =>{%>
            <tr>
                <td> <%= row.patient_id %> </td>
                <td> <%= row.first_name %> </td>
                <td> <%= row.last_name %> </td>
                <td> <%= row.Date of Birth %> </td>
                <td> <%= row.Gender %> </td>
                <td> <%= row.language %> </td>

            </tr>
        <%});%>
    </tbody>
</table>